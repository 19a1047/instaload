{
  "manifest_version": 3,
  "name": "Instagram Image Link Scraper",
  "version": "1.1",
  "description": "Floating button to scrape all images from Instagram profile/page.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://www.instagram.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["content.js"]
    }
  ]
}
